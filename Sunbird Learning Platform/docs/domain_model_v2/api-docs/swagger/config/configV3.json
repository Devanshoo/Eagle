  "paths": {
    "/resourcebundles/list": {
      "get": {
        "summary": "Gets All resourcebundles",
        "description": "This API returns all the resourcebundles for languages supported by the EkStep Platform",
        "tags": [
          "ResourceBundle"
        ],
        "operationId": "ekstep.config.resourebundles.list",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "All resourcebundles Returned Successfully",
            "schema": {
              "$ref": "#/definitions/getAllResourcebundlesResponse"
            }
          },
          "400": {
            "description": "Error While fetching for all resourcebundles! | Error occurred while operation, mostly because of the input given by client, In general it is 'Client Exception'",
            "schema": {
              "$ref": "#/definitions/FailedResponseHeader"
            }
          },
          "500": {
            "description": "Error While fetching for all resourcebundles! | Error occurred while operation, mostly because of erroneous condition on server, In general it is 'Server Exception' or 'Exception'",
            "schema": {
              "$ref": "#/definitions/FailedResponseHeader"
            }
          }
        },
        "security": []
      }
    },
    "/resourcebundles/read/{languageId}": {
      "get": {
        "summary": "Gets Specific resourcebundle",
        "description": "This API returns specific resourcebundle based on the specified language",
        "tags": [
          "ResourceBundle"
        ],
        "operationId": "ekstep.config.resourcebundles.find",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "languageId",
            "in": "path",
            "required": true,
            "description": "languageId",
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "ResourcebundleJson of specified language Returned Successfully",
            "schema": {
              "$ref": "#/definitions/getResourcebundleResponse"
            }
          },
          "400": {
            "description": "Error While fetching for resourcebundle of specified language! | Error occurred while operation, mostly because of the input given by client, In general it is 'Client Exception'",
            "schema": {
              "$ref": "#/definitions/FailedResponseHeader"
            }
          },
          "500": {
            "description": "Error While fetching for definition of specified language! | Error occurred while operation, mostly because of erroneous condition on server, In general it is 'Server Exception' or 'Exception'",
            "schema": {
              "$ref": "#/definitions/FailedResponseHeader"
            }
          }
        },
        "security": []
      }
    },
    "/ordinals/list": {
      "get": {
        "summary": "Gets Config ordianls",
        "description": "This API returns list of config ordinals supported by the platform",
        "tags": [
          "Ordinals"
        ],
